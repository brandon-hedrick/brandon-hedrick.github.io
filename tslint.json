{
  "defaultSeverity": "error",
  "extends": [
      "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
      "interface-name": false,
      "member-access": [
          false
      ],
      "ordered-imports": [
          false
      ],
      "no-console": [
          true
      ],
      "no-var-requires": false,
      "quotemark": [true, "single", "jsx-double", "avoid-escape"]
  },
  "rulesDirectory": []
}